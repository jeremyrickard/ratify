{
    "executor": {
        "executionMode": "passthrough"
    },
    "store": {
        "version": "1.0.0",
        "plugins": [
            {
                "name": "oras"
            }
        ]
    },
    "policy": {
        "version": "1.0.0",
        "plugin": {
            "name": "configPolicy",
            "artifactVerificationPolicies": {
                "application/vnd.cncf.notary.v2.signature": "any"
            }
        }
    },
    "verifier": {
        "version": "1.0.0",
        "plugins": [
            {
                "name": "packagechecker",
                "artifactTypes": "application/spdx+json",
                "disAllowedPackages": [
                    {
                        "name": "openssl",
                        "version": "3.0.5-r3"
                    }
                ]
            }
        ]
    }
}